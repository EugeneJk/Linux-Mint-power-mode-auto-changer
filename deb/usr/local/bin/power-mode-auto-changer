#!/bin/bash
MAIN_SCRIPTS_DIR="/usr/local/lib/power-mode-auto-changer"

# Extract params
source "$MAIN_SCRIPTS_DIR/scripts-common/extract-args.sh"

# Define lang
source "$MAIN_SCRIPTS_DIR/scripts-pmac/define-current-lang.sh"

#connect translations 
source "$MAIN_SCRIPTS_DIR/scripts-pmac/lang/$LANG_SELECTED.conf"

#run configurator 
source "$MAIN_SCRIPTS_DIR/scripts-pmac/configurator.sh"


